{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["src/main.tsx"],
  "project": [
    "src/**/*.{ts,tsx}",
    "!src/**/*.test.{ts,tsx}",
    "!src/**/*.spec.{ts,tsx}"
  ],
  "ignore": [
    "src/test/**",
    "src/**/*.test.{ts,tsx}",
    "src/**/*.spec.{ts,tsx}",
    "**/*.d.ts"
  ],
  "ignoreDependencies": ["vite-bundle-analyzer"],
  "ignoreBinaries": ["depcheck", "commitlint"],
  "ignoreExportsUsedInFile": true,
  "includeEntryExports": true
}
