{
  "name": "modern-react-stack",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:quality": "npm run quality:pre-build && tsc && vite build && npm run quality:post-build",
    "build:analyze": "npm run build && npm run analyze",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "prepare": "husky install",
    "commit": "cz",
    "analyze": "npx vite-bundle-analyzer dist/assets/*.js",
    "deadcode": "knip",
    "deadcode:fix": "knip --fix",
    "deps:check": "npm-check-updates",
    "deps:update": "npm-check-updates -u",
    "deps:audit": "npm audit --audit-level=moderate",
    "size:check": "size-limit",
    "size:why": "size-limit --why",
    "deps:unused": "depcheck",
    "deps:circular": "madge --circular --extensions ts,tsx src/",
    "deps:graph": "madge --image deps-graph.png src/",
    "quality:pre-build": "npm run type-check && npm run lint && npm run test -- --run && npm run deadcode",
    "quality:post-build": "npm run size:check && npm run deps:circular",
    "quality:full": "npm run quality:pre-build && npm run build && npm run quality:post-build",
    "clean": "npm run deadcode && npm run lint && npm run type-check && npm run test",
    "ci": "npm run quality:full",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build:all": "npm run build && npm run build-storybook",
    "quality:storybook": "npm run build-storybook"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@tanstack/react-query": "^5.90.5",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.38.0",
    "@size-limit/preset-small-lib": "^11.2.0",
    "@storybook/addon-a11y": "^9.1.13",
    "@storybook/addon-docs": "^9.1.13",
    "@storybook/addon-onboarding": "^9.1.13",
    "@storybook/react-vite": "^9.1.13",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^24.8.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-storybook": "^9.1.13",
    "husky": "^9.1.7",
    "jsdom": "^27.0.1",
    "knip": "^5.66.0",
    "markdown-to-jsx": "^7.4.2",
    "lint-staged": "^16.2.4",
    "madge": "^8.0.0",
    "msw": "^2.11.5",
    "npm-check-updates": "^19.1.1",
    "postcss": "^8.5.6",
    "prettier": "^3.3.3",
    "size-limit": "^11.2.0",
    "storybook": "^9.1.13",
    "typescript": "^5.6.3",
    "vite": "^7.1.10",
    "vite-bundle-analyzer": "^1.2.3",
    "vitest": "^3.2.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
